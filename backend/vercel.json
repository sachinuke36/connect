{
    "version": 2,
    "builds": [
      {
        "src": "backend/**/*.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/$1"
      },
        {
            "src": "/(.*)",
            "dest": "/backend/$1"
          }
    ]
  }
  